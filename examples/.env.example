# Default provider (openai)
OPENAI_API_KEY=

# Alternative providers â€” uncomment the one you use
# ANTHROPIC_API_KEY=
# GOOGLE_API_KEY=
# GROQ_API_KEY=
# MISTRAL_API_KEY=
# CO_API_KEY=

# OpenRouter (use with base_url + api_key_env in role.yaml)
# OPENROUTER_API_KEY=

# Embedding providers reuse the same API keys as LLM providers above.
# For example, Google embeddings use GOOGLE_API_KEY.
# For custom embedding endpoints, set api_key_env in your role.yaml's
# ingest.embeddings section to point to the env var holding the key.

# GitHub registry access (optional, increases rate limits)
# GITHUB_TOKEN=

# Slack integration (used by deployment-checker, slack-echo, deploy-notifier, etc.)
# SLACK_WEBHOOK_URL=

# Data directory override (default: ~/.initrunner)
# INITRUNNER_HOME=
